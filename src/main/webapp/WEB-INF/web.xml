<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Projet AAR - Evaluator8000 (Arnault - Lebrument)</display-name>
  
  <servlet>
  	<servlet-name>evalServlet</servlet-name>
  	<servlet-class>evaluator.servlets.MediaEvaluatorServlet</servlet-class>
  </servlet>
  
  <servlet>
  	<servlet-name>findServlet</servlet-name>
  	<servlet-class>evaluator.servlets.MediaFinderServlet</servlet-class>
  </servlet>
  
  <servlet>
  	<servlet-name>searchServlet</servlet-name>
  	<servlet-class>evaluator.servlets.MediaSearcherServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>evalServlet</servlet-name>
  	<url-pattern>/evalMedia</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>findServlet</servlet-name>
  	<url-pattern>/findMedia</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>searchServlet</servlet-name>
  	<url-pattern>/searchMedia</url-pattern>
  </servlet-mapping>
  
</web-app>
